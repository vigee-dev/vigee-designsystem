{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "strict": true,
    "esModuleInterop": true,
      "module": "esnext",
    "declaration": true,
    "outDir": "./lib",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "skipLibCheck": true
  },
   "paths": {
      "@/*": ["./*"]
    },
  "include": ["./**/*"],
  "exclude": ["node_modules", "lib"]
}
